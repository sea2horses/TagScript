# Evaluate the number
<if>
    <condition>
        <compare>
            <input prompt="Type a number:" target-type="number"/>
            [5] 
        </compare>
    </condition>
    <output>
        "Your number is 5!"
    </output>
</if>
<else>
    <output>
        "Your number is not 5!"
    </output>
</else>